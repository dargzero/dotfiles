# host and port
set connect.host 192.168.0.100
set connect.port 9091

# important: create ./creds.conf containing transmission credentials:
   # set connect.user username
   # set connect.password password

# import creds.conf
rc creds.conf

# poll frequency (seconds)
set tui.poll 10

# default columns in torrent list
set columns.torrents name %dn eta ratio rate-up rate-down

